{
  "name": "react-reducks",
  "version": "1.0.0-beta.8",
  "description": "A lightweight and powerful Redux kit to easily manage state in React applications",
  "main": "index.js",
  "repository": "git@github.com:phixo/react-reducks.git",
  "contributors": [
    "Maurice Bartnig <maurice.bartnig@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "async",
    "hooks",
    "reducer",
    "functional",
    "toolkit"
  ],
  "jest": {
    "cacheDirectory": "./node_modules/.jest-cache",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.js"
    ]
  },
  "release-script": {
    "altPkgRootFolder": "lib",
    "defaultDryRun": "true"
  },
  "scripts": {
    "build": "yarn run clean && babel src --out-dir lib && cp LICENSE README.md ./lib",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "release",
    "release:alpha": "release --preid alpha",
    "release:next": "release --preid beta --tag next",
    "test": "jest",
    "test:ci": "yarn run test && codecov",
    "watch:test": "jest --watchAll --coverage"
  },
  "peerDependencies": {
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "axios": "0.18.0",
    "axios-mock-adapter": "1.16.0",
    "babel-eslint": "10.0.1",
    "codecov": "3.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.15.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "path-to-regexp": "3.0.0",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-hooks-testing-library": "0.3.6",
    "react-test-renderer": "16.8.3",
    "reselect": "^4.0.0",
    "rimraf": "2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && yarn lint"
    }
  }
}
